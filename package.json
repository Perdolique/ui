{
  "name": "ui",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "description": "UI components for various projects",
  "author": "Roman Nuritdinov (Perdolique)",
  "license": "UNLICENSED",
  "keywords": [
    "ui",
    "components",
    "library",
    "vue",
    "reka-ui"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "update:all": "taze -r -l -I latest",
    "update:newest": "taze -r -l -I newest",
    "lint:styles": "stylelint 'src/**/*.vue' 'src/**/*.css'",
    "lint:oxlint": "oxlint",
    "lint:eslint": "eslint",
    "lint:all": "pnpm run --parallel '/^lint:/'",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.2.8",
    "vue": "3.5.26"
  },
  "devDependencies": {
    "@iconify-json/streamline": "1.2.5",
    "@iconify-json/tabler": "1.2.26",
    "@iconify/vue": "5.0.0",
    "@stylistic/eslint-plugin": "5.6.1",
    "@types/node": "25.0.3",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.39.2",
    "eslint-config-greenpie": "14.17.0",
    "eslint-plugin-vue": "10.6.2",
    "husky": "9.1.7",
    "oxlint": "1.36.0",
    "reka-ui": "2.7.0",
    "stylelint": "16.26.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.1",
    "taze": "19.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "unplugin-icons": "22.5.0",
    "vite": "8.0.0-beta.5",
    "vue-tsc": "3.2.1"
  },
  "packageManager": "pnpm@10.26.1"
}
